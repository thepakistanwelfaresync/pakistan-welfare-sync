    <style>
        * {
            font-family: 'Inter', sans-serif;
        }

        /* Anti-Gravity Floating Icons */
        .floating-icon {
            position: absolute;
            opacity: 0.15;
            pointer-events: none;
            animation: float 20s infinite ease-in-out;
        }

        @keyframes float {

            0%,
            100% {
                transform: translate(0, 0) rotate(0deg);
            }

            25% {
                transform: translate(30px, -40px) rotate(90deg);
            }

            50% {
                transform: translate(-20px, -80px) rotate(180deg);
            }

            75% {
                transform: translate(-40px, -40px) rotate(270deg);
            }
        }

        .floating-icon:nth-child(1) {
            top: 10%;
            left: 10%;
            animation-duration: 18s;
            animation-delay: 0s;
        }

        .floating-icon:nth-child(2) {
            top: 60%;
            left: 15%;
            animation-duration: 22s;
            animation-delay: 2s;
        }

        .floating-icon:nth-child(3) {
            top: 30%;
            right: 20%;
            animation-duration: 20s;
            animation-delay: 4s;
        }

        .floating-icon:nth-child(4) {
            top: 70%;
            right: 15%;
            animation-duration: 24s;
            animation-delay: 1s;
        }

        .floating-icon:nth-child(5) {
            top: 45%;
            left: 50%;
            animation-duration: 19s;
            animation-delay: 3s;
        }

        .floating-icon:nth-child(6) {
            top: 20%;
            right: 40%;
            animation-duration: 21s;
            animation-delay: 5s;
        }

        .floating-icon:hover {
            animation-play-state: paused;
            opacity: 0.4;
            transform: scale(1.2);
            transition: all 0.3s ease;
        }

        /* Smooth scroll */
        html {
            scroll-behavior: smooth;
        }

        /* Gradient text */
        .gradient-text {
            background: linear-gradient(135deg, #1E5631 0%, #3ECFBB 100%);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
        }

        /* Card hover effects */
        .tech-card {
            transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);
        }

        .tech-card:hover {
            transform: translateY(-10px);
            box-shadow: 0 20px 40px rgba(30, 86, 49, 0.2);
        }

        /* Timeline step animation */
        .timeline-step {
            transition: all 0.3s ease;
        }

        .timeline-step:hover {
            transform: scale(1.05);
        }

        /* Button pulse effect */
        @keyframes pulse {

            0%,
            100% {
                box-shadow: 0 0 0 0 rgba(62, 207, 187, 0.7);
            }

            50% {
                box-shadow: 0 0 0 15px rgba(62, 207, 187, 0);
            }
        }

        .btn-pulse {
            animation: pulse 2s infinite;
        }
    </style>
</head>

